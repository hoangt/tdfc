#!/bin/csh
#
# - Extract cumulative build statistics from xilinx ``make.OUT''
#
# - ``make.OUT'' is generated by ``xilinx-makefile-63i'',
#     which may recurse to iterate
# - These cumulative stats must be collected separately
#     after build is complete
#
# - Eylon Caspi 9/1/05

set toolpath = ${0:h}

if ( (-f make.OUT) && (-f PERIOD_PAR) ) then

  rm -f EXPANSIONS_MAP EXPANSIONS_PAR
  ${toolpath}/xilinx-expansions-map-63i make.OUT > EXPANSIONS_MAP
  ${toolpath}/xilinx-expansions-par-63i make.OUT > EXPANSIONS_PAR

  rm -f TIME_PAR_CUMU TIME_PAR_MAX
  ${toolpath}/xilinx-time-par-cumu-63i  make.OUT > TIME_PAR_CUMU
  ${toolpath}/xilinx-time-par-max-63i   make.OUT > TIME_PAR_MAX

endif
