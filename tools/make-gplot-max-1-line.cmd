perl -e 'foreach $file (<mix=*>) { print "opening $file\n"; open(IN,"zcat $file |"); $cut_df_bits_max=0; $cut_df_streams_max=0; foreach (<IN>) { if (/cut_df_bits=(\d+)/ && $cut_df_bits_max<$1) {$cut_df_bits_max=$1} elsif (/cut_df_streams=(\d+)/ && $cut_df_streams_max<$1) {$cut_df_streams_max=$1} close(IN) } print "maxima:  cut_df_bits=$cut_df_bits_max cut_df_streams=$cut_df_streams_max\n"; open(BITS,">> results-bits-max.gplot"); printf(BITS "%1.1f %d\n",$mix,$cut_df_bits_max); close(BITS); open(STREAMS,">> results-streams-max.gplot"); printf(STREAMS "%1.1f %d\n",$mix,$cut_df_streams_max); close(STREAMS); $mix+=0.1 }'
